{
  "name": "image-color-picker",
  "displayName": "Image Color Picker",
  "description": "Pick colors from images in VS Code and save hex color codes",
  "publisher": "KEHEMIT",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "color",
    "picker",
    "image",
    "hex",
    "color-picker",
    "image-color",
    "design",
    "web-development"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KEHEM-IT/image-color-picker.git"
  },
  "homepage": "https://github.com/KEHEM-IT/image-color-picker",
  "bugs": {
    "url": "https://github.com/KEHEM-IT/image-color-picker/issues"
  },
  "license": "MIT",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "imageColorPicker.pickColor",
        "title": "Pick Color from Image",
        "category": "Image Color Picker"
      },
      {
        "command": "imageColorPicker.pickColorFromFile",
        "title": "Pick Color from Image File...",
        "category": "Image Color Picker"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "imageColorPicker.pickColor",
          "when": "resourceExtname =~ /\\.(png|jpg|jpeg|gif|bmp|svg|webp)$/i",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "imageColorPicker.pickColor",
          "when": "resourceExtname =~ /\\.(png|jpg|jpeg|gif|bmp|svg|webp)$/i",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "imageColorPicker.pickColor",
          "when": "resourceExtname =~ /\\.(png|jpg|jpeg|gif|bmp|svg|webp)$/i",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "^16.18.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {}
}
